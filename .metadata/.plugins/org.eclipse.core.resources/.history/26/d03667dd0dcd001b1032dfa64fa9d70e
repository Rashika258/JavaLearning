class Emp {
	int id;
	String name;
	int age;
	
	public void show() {
		System.out.println(id);
		System.out.println(name);
		System.out.println(age);
	}
	
}
public class ObjectDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Emp obj=new Emp();
		obj.id=1;
		obj.name="Rashika";
		obj.age=21;
		obj.show();
		System.out.println(obj);
		
		
		
	}

}
