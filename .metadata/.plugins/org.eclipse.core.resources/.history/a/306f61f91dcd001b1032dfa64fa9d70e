class Emp3{
	int id;
	String name;
	static String ceo;
	//static variables aren't object specific
	public void show() {
		// TODO Auto-generated method stub
		System.out.println(name + ceo+ id);
	}
	
}
public class StringDemo {
	public static void main(String[] args) {
		Emp3 n=new Emp3();
		n.id=8;
		n.name="rashi";
		n.ceo="Mahesh";
		
		Emp3 r=new Emp3();
		r.id=9;
		r.name="rashika";
		r.ceo="Mahesh";
		
		n.show();
		r.show();
		
	}
}
